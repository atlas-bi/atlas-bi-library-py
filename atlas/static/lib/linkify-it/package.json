{
  "name": "linkify-it",
  "version": "3.0.27",
  "description": "Links recognition library with FULL unicode support",
  "keywords": [
    "linkify",
    "linkifier",
    "autolink",
    "autolinker"
  ],
  "repository": "markdown-it/linkify-it",
  "files": [
    "index.js",
    "lib/"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && nyc mocha",
    "coverage": "npm run test && nyc report --reporter html",
    "report-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "demo": "npm run lint && node support/build_demo.js",
    "doc": "node support/build_doc.js",
    "gh-pages": "npm run demo && npm run doc && shx cp -R doc/ demo/ && gh-pages -d demo -f",
    "prepublishOnly": "npm run gh-pages"
  },
  "dependencies": {
    "uc.micro": "1.0.6"
  },
  "devDependencies": {
    "ansi": "0.3.1",
    "autoprefixer-stylus": "1.0.0",
    "benchmark": "2.1.4",
    "browserify": "17.0.0",
    "coveralls": "3.1.1",
    "eslint": "8.37.0",
    "gh-pages": "4.0.0",
    "mdurl": "1.0.1",
    "mocha": "10.2.0",
    "ndoc": "6.0.0",
    "nyc": "15.1.0",
    "pug-cli": "1.0.0-alpha6",
    "shelljs": "0.8.5",
    "shx": "0.3.4",
    "stylus": "0.59.0",
    "tlds": "1.238.0"
  }
}
